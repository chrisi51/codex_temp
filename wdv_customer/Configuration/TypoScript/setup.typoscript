config {
    sys_language_mode = ignore

    # cache remains 15 minutes (900 seconds)
    cache_period = 900

    recordLinks {
        artikel {
            forceLink = 1
            typolink {
                parameter.stdWrap.cObject = CONTENT
                parameter.stdWrap.cObject {
                    table = sys_category
                    select {
                        orderBy = sys_category.single_pid ASC
                        pidInList = 107
                        selectFields = sys_category.single_pid as detailPid
                        join = sys_category_record_mm ON sys_category_record_mm.uid_local = sys_category.uid
                        where = sys_category_record_mm.uid_foreign = {field:uid} AND sys_category.single_pid != 0
                        where.insertData = 1
                        max = 1
                    }

                    renderObj = TEXT
                    renderObj.field = detailPid
                    renderObj.wrap = |
                }

                additionalParams.data = field:uid
                additionalParams.wrap = &tx_news_pi1[news]=|
            }
        }

        popup {
            forceLink = 1
            typolink {
                parameter.data = field:uid
                parameter.insertData = 1
                additionalParams.data = field:uid
                additionalParams.wrap = &id=|&type=1968
                addQueryString = 1
            }
        }
    }
}

