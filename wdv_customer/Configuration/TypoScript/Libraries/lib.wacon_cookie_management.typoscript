lib.wacon_cookie_management = COA
lib.wacon_cookie_management {
    // Add cookie-management entry
    10 = COA
    10 {

        20 < tt_content.list.20.waconcookiemanagement_cookiefreigabe
        20 {

            switchableControllerActions {
                Cookie {
                    1 = list
                }
            }
            view < plugin.tx_waconcookiemanagement_cookiefreigabe.view
            features < plugin.tx_waconcookiemanagement_cookiefreigabe.features
            mvc < plugin.tx_waconcookiemanagement_cookiefreigabe.mvc

            persistence {

                storagePid = 462
            }
            settings {

                nurLink = 0
                dataProtection = 130
                imprint = 132
            }
        }
    }
}

