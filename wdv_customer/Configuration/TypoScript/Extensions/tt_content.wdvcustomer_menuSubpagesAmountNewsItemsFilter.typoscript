
# Menu of subpages of selected pages with amout of news items within same category:
tt_content.wdvcustomer_menuSubpagesAmountNewsItemsFilter =< lib.contentElement
tt_content.wdvcustomer_menuSubpagesAmountNewsItemsFilter {
    templateName = MenuSubpagesAmountNewsItemsFilter
    dataProcessing {
        #10 = WDV\WdvCustomer\DataProcessing\MenuSubpagesAmountNewsItemsFilterProcessor
        10 = TYPO3\CMS\Frontend\DataProcessing\MenuProcessor
        10 {
            special = directory
            special.value.field = pages
        }
    }
    stdWrap {
        editIcons = tt_content: header [header_layout], pages
        editIcons {
            iconTitle.data = LLL:EXT:fluid_styled_content/Resources/Private/Language/FrontendEditing.xlf:editIcon.menu
        }
    }
}
