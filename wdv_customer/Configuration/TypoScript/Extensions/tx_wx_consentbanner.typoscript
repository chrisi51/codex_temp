        templateRootPaths.10 = EXT:wdv_customer/Resources/Private/Extensions/wxconsentbanner/Templates/
        layoutRootPaths.10 = EXT:wdv_customer/Resources/Private/Extensions/wxconsentbanner/Layouts/
        partialRootPaths.10 = EXT:wdv_customer/Resources/Private/Extensions/wxconsentbanner/Partials/
    }
}

lib.wx_consentbanner = COA
lib.wx_consentbanner {
    10 {
        persistence {
            # folder id where your cookies are stored
            storagePid = 500
        }

        settings {
            # delete all cookies, which are not managed or are disallowed by website user
            removeBadCookies = 0
            # url or page uid to your privacy policy and imprint
            url_privacypolicy = 499
            url_imprint = 497
        }
    }
}

